<!DOCTYPE html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include("signup.css.ejs"); %>
    
</head>
<body>
    <div class="page">
        <div class="header">
            <h1 class="title">WeirdWebForum - login</h1>
        </div>
        <div class="messageContainer" id="messageContainer">
            <h2 class="loginHead">Create a WF account</h2>
            <input id="usernameIn" class="input" placeholder="Username">
            <input id="passwordIn" class="input" placeholder="Password" type="password">
            <button class="signupBtn" onclick="signup()">Create account</button>
            <a class="login" href="../login">Log in here</a>
        </div>
        <div class="footer">

        </div>
    </div>
    <%- include("signup.js.ejs"); %>
</body>
</html>